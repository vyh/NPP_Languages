<NotepadPlus>
    <UserLang name="LC4" ext="asm">
        <Settings>
            <Global caseIgnored="yes" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">000000</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators">,</Keywords>
            <Keywords name="Comment">1 2 0;</Keywords>
            <Keywords name="Words1">.DATA .CODE .ADDR .FALIGN .FILL .BLKW .CONST .UCONST .OS RET LEA LC</Keywords>
            <Keywords name="Words2">NOP BRn BRnz BRnp BRz BRzp BRp BRnzp ADD MUL SUB DIV CMP CMPU CMPI CMPIU JSR JSRR AND NOT OR XOR LDR STR RTI CONST SLL SRA SRL MOD JMPR JMP HICONST TRAP</Keywords>
            <Keywords name="Words3">R0 R1 R2 R3 R4 R5 R6 R7</Keywords>
            <Keywords name="Words4"># x</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="CAF4F4" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="000000" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="000000" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="8080C0" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="A3A3D1" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="478D8D" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="8DC7C7" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="C0C0C0" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="C0C0C0" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="2" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="8DC7C7" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="C0C0C0" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="C0C0C0" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="C0C0C0" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
